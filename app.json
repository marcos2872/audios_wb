{
  "expo": {
    "name": "audios_wb",
    "slug": "audios_wb",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/cover.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/cover.png",
      "resizeMode": "contain",
      "backgroundColor": "#191414"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/cover.png",
        "backgroundColor": "#191414"
      }
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/images/favicon.png"
    }
  }
}
